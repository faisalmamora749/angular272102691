{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular272102691": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "src/styles.css",

              "src/assets/admin-lte/dist/css/adminlte.min.css",
              "src/assets/admin-lte/plugins/fontawesome-free/css/all.min.css",

              "src/assets/admin-lte/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css",
              "src/assets/admin-lte/plugins/datatables-responsive/css/responsive.bootstrap4.min.css",
              "src/assets/admin-lte/plugins/datatables-buttons/css/buttons.bootstrap4.min.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",

              "src/assets/admin-lte/plugins/datatables/jquery.dataTables.min.js",
              "src/assets/admin-lte/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js",
              "src/assets/admin-lte/plugins/datatables-responsive/js/dataTables.responsive.min.js",
              "src/assets/admin-lte/plugins/datatables-responsive/js/responsive.bootstrap4.min.js",
              "src/assets/admin-lte/plugins/datatables-buttons/js/dataTables.buttons.min.js",
              "src/assets/admin-lte/plugins/datatables-buttons/js/buttons.bootstrap4.min.js",
              "src/assets/admin-lte/plugins/jszip/jszip.min.js",
              "src/assets/admin-lte/plugins/pdfmake/pdfmake.min.js",
              "src/assets/admin-lte/plugins/pdfmake/vfs_fonts.js",
              "src/assets/admin-lte/plugins/datatables-buttons/js/buttons.html5.min.js",

              "src/assets/admin-lte/dist/js/adminlte.min.js"

            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "serviceWorker": "ngsw-config.json"
            },
            "development": {
              "optimization": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "development"
        },

        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "angular272102691:build:production"
            },
            "development": {
              "buildTarget": "angular272102691:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  }
}
